MEMORY 41 2 2 0
PROC PRODUCTOR
MOV CX 3
P 1
MOVV AX 0
ADD AX 5
MOVRV 0 AX
NOP
NOP
V 2
DEC CX
CMP CX 0
JNE 3
ENDP
PROC CONSUMIDOR
MOV CX 3
P 2
MOVV EX 0
MOVRV 1 EX
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
V 1
DEC CX
CMP CX 0
JNE 16
ENDP
BEGIN
SET 1 1
SET 2 0
PARBEGIN
CALL PRODUCTOR
CALL CONSUMIDOR
PAREND
END