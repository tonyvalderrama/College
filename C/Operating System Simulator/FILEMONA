MEMORY 60 2 0 2
INIMO
PROCM PRODUCTOR
MOVV BX 1
CMP BX 1
JNE 7
WAIT 0
MOV AX 10
MOVRV 2 AX
INC BX
MOVRV 1 BX
CMP BX 1
JNE 14
SIGNAL 1
ENDM
PROCM CONSUMIDOR
MOVV BX 1
CMP BX 0
JNE 20
WAIT 1
MOV AX 0
MOVRV 2 AX
DEC BX
MOVRV 1 BX
CMP BX 0
JNE 27
SIGNAL 0
ENDM
PROCM INICIO
MOV AX 1
MOVRV 1 AX
ENDM
FINMO
PROC LECTOR
MOV CX 3
CALLM CONSUMIDOR
DEC CX
CMP CX 0
JNE 35
ENDP
PROC ESCRITOR
MOV CX 3
CALLM PRODUCTOR
DEC CX
CMP CX 0
JNE 42
ENDP
BEGIN
PARBEGIN
CALL LECTOR
CALL ESCRITOR
PAREND
NOP
GET 2 4
NOP
NOP
FRE 2 4
NOP
NOP
END


